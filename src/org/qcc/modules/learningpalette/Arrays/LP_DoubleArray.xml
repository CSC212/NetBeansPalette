<PaletteItem>
    <Category>Arrays</Category>
    <DisplayName>Double Array</DisplayName>
    <Tooltip>A double array is an array consisting of doubles,\n very large numbers that can contain decimals.\nIf providing data, separate it with a space, and do not provide a size.</Tooltip>
    <Example>double[] varName = { 1233412, -3.03945, 830.23, 0 }</Example>
    <Icon>org/qcc/modules/learningpalette/Arrays/array16.png</Icon>
    <Languages>
        <Language id="JAVA">
            <codeTemplate1>double[] $varName;</codeTemplate1>
            <codeTemplate2>double[] $varName = new double[$size];</codeTemplate2>
            <codeTemplate3>double[] $varName = {$initializeValue};</codeTemplate3>
        </Language>
        <Language id="CPP">
            <codeTemplate1>double $varName[$size];</codeTemplate1>
            <codeTemplate2>double $varName[$size];</codeTemplate2>
            <codeTemplate3>double $varName[] = {$initializeValue};</codeTemplate3>
        </Language>
    </Languages>
    <!-- Customizer dynamic creation at runtime, currently supported controls:
        Text, Combo
    -->
    <Customizer>
        <Item type="Text" name="VariableName" label="Array Name:" value="" />
        <Item type="Text" name="ArraySize" label="Array Size:" value="" />
        <Item type="Text" name="InitializeValue" label="Array Initialized Value:" value="" />
        <Item type="Text" name="Comment" label="Comment:" value="" />
    </Customizer>
    <PaletteLogicClass>org.qcc.modules.learningpalette.Arrays.LP_DoubleArray</PaletteLogicClass>
</PaletteItem>