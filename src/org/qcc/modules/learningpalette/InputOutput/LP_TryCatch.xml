<PaletteItem>
    <Category>Input Output</Category>
    <DisplayName>Try Catch</DisplayName>
    <Tooltip>This is an try/catch block,\n it will catch any errors your code may throw and stop execution.</Tooltip>
    <Example>try { code to try here } catch (Exception ex) { code to run if an exception is thrown }</Example>
    <Icon>org/qcc/modules/learningpalette/InputOutput/error.png</Icon>
    <Languages>
        <Language id="JAVA">
            <codeTemplate1>//You can also add multiple catch statements for different types of exceptions.
                try {
                //TODO: Your code here
                } catch ($ExceptionType ex) {
                System.out.println(ex.toString());
                } finally {
                //TODO: Anything you want to do after your try or catch statement
                }</codeTemplate1>
        </Language>
        
        <Language id="CPP">
            <codeTemplate1>//You can also add multiple catch statements for different types of exceptions.
                try {
                //TODO: Your code here
                } catch ($ExceptionType ex) {
                cout &gt;&gt; ex;
                } finally {
                //TODO: Anything you want to do after your try or catch statement
                }</codeTemplate1>
        </Language>
    </Languages>
