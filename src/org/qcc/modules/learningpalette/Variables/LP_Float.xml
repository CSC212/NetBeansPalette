<PaletteItem>
    <Category>Variables</Category>
    <DisplayName>Float</DisplayName>
    <Tooltip>This is a float variable.\nIt stores very precise numbers.</Tooltip>
    <Example>A float will appear in your code as follows:\nfloat variableName = -3.12032319;</Example>
    <Icon>org/qcc/modules/learningpalette/Variables/object16.png</Icon>
    <Languages>
        <Language id="JAVA">
            <codeTemplate1>float $varName;</codeTemplate1>
            <codeTemplate2>float $varName = $valuef;</codeTemplate2>

        </Language>
        <Language id="CPP">
            <codeTemplate1>float $varName;</codeTemplate1>
            <codeTemplate2>float $varName = $value;</codeTemplate2>

        </Language>
    </Languages>
    <!-- Customizer dynamic creation at runtime, currently supported controls:
        Text, Combo
