<PaletteItem>
    <Category>Variables</Category>
    <DisplayName>Long</DisplayName>
    <Tooltip>This is a long variable.\nIt stores large positive and negative whole numbers.\nMuch larger than an integer (32 bit vs 64 bit).</Tooltip>
    <Example>A long will appear in your code as follows:\nlong variableName = 123812894712841223;</Example>
    <Icon>org/qcc/modules/learningpalette/Variables/object16.png</Icon>
    <Languages>
        <Language id="JAVA">
            <codeTemplate1>long $varName;</codeTemplate1>
            <codeTemplate2>long $varName = $value;</codeTemplate2>

        </Language>
        <Language id="CPP">
            <codeTemplate1>long int $varName;</codeTemplate1>
            <codeTemplate2>long int $varName = $value;</codeTemplate2>

        </Language>
    </Languages>
    <!-- Customizer dynamic creation at runtime, currently supported controls:
        Text, Combo
    -->
    <Customizer>
        <Item type="Text" name="VariableName" label="Variable Name:" value="" />
        <Item type="Text" name="DefaultValue" label="Default Value:" value="" />
        <Item type="Text" name="Comment" label="Comment:" value="" />
    </Customizer>
    <PaletteLogicClass>org.qcc.modules.learningpalette.Variables.LP_Long</PaletteLogicClass>
</PaletteItem>